auto lo
iface lo inet loopback

auto ens192
iface ens192 inet static
    address 10.0.45.11
    netmask 255.255.252.0
    network 10.0.44.0
    broadcast 10.0.47.255
    gateway 10.0.44.4
    # dns-* options are implemented by the resolvconf package, if installed
    dns-nameservers 10.0.45.11
    dns-search vaai
